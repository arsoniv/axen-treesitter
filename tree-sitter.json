{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "axenlang",
      "camelcase": "Axenlang",
      "title": "Axenlang",
      "scope": "source.axenlang",
      "path": ".",
      "file-types": [
        "ax"
      ],
      "highlights": "queries/highlights.scm",
      "class-name": "TreeSitterAxenlang",
      "injection-regex": "ax"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Axenlang grammar for tree-sitter",
    "authors": [
      {
        "name": "Arsoniv",
        "email": "arsonivalt@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/arsoniv/axen-treesitter"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
